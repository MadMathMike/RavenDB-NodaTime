<Project Sdk="Microsoft.NET.Sdk">
  
  <PropertyGroup>
    <PackageId>RavenDB.Client.NodaTime</PackageId>
    <Title>Noda Time support for RavenDB</Title>
    <Description>Provides NodaTime support for later versions of the RavenDB Client. Forked from https://github.com/mj1856/RavenDB-NodaTime.</Description>
    <Authors>Michael Peterson</Authors>
    <PackageTags>ravendb;raven db;noda time;nodatime</PackageTags>
    <PackageProjectUrl>https://github.com/MadMathMike/RavenDB-NodaTime</PackageProjectUrl>
    <PackageLicenseUrl>https://raw.github.com/MadMathMike/RavenDB-NodaTime/master/LICENSE.txt</PackageLicenseUrl>
    <PackageIconUrl>http://static.ravendb.net/logo-for-nuget.png</PackageIconUrl>
    <IncludeSource>True</IncludeSource>
    <IncludeSymbols>True</IncludeSymbols>
    <Version>4.0.0</Version>
    <TargetFrameworks>netstandard1.3</TargetFrameworks>
  </PropertyGroup>
  
  <PropertyGroup>
    <TargetFramework>netstandard1.3</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="NodaTime" Version="2.2.4" />
    <PackageReference Include="RavenDB.Client" Version="4.0.3" />
  </ItemGroup>

</Project>
